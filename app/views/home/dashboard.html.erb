
<div  class="section_heading">
<h1 class= "section-heading" style="color:brown;font-size:300%;font-family:'Contrail One',cursive;">My Dashboard</h1>

<i class="fa fa-user-circle fa-3x" aria-hidden="true"></i>

<table id="dashboard_style">
  <thead>
    <tr>
 <th class= "section-heading" style="color:black;font-size:100%;font-family:'Contrail One',cursive;">My Boat Fleet</th>

 <% @boats.each do |boat| %>

   <th><%= boat.name %></th>
   <% end %>
</tr>
<tr>
       <th class= "section-heading" style="color:black;font-size:100%;font-family:'Contrail One',cursive;">My Shipments</th>


       	<% @jobs.each do |jobs| %>

       		<th><%= jobs.id %></th>
       		<th><%= jobs.status %></th>
       		<% end %>
</tr>
<tr>

<%= current_company.email %>
 <th colspan="3"></th>
   </tr>
 </thead>
</div>
 <tbody>
</tbody>
</table>

<%= link_to new_job_path do %>

<span class="fa-stack fa-4x" style="color:brown;">
<i class="fa fa-anchor" aria-hidden="true"></i>
<button id="newBoatbutton" class="btn btn-primary btn-xl text-uppercase" type="submit">New Shipment</button>

</span>

<% end %>

<%= link_to new_boat_path do %>

        <span class="fa-stack fa-4x" style="color:brown;">
        <i class="fa fa-anchor" aria-hidden="true"></i>
  <button id="newBoatbutton" class="btn btn-primary btn-xl text-uppercase" type="submit">New Boat</button>

        </span>

        <% end %>

